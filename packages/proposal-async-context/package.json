{
  "name": "@quisido/proposal-async-context",
  "version": "0.0.2",
  "author": "Chengzhong Wu <legendecas@gmail.com>",
  "description": "Async Context proposal for JavaScript",
  "homepage": "https://github.com/tc39/proposal-async-context",
  "license": "CC0-1.0",
  "main": "./dist/src/index.js",
  "module": "./dist/src/index.js",
  "type": "module",
  "types": "./dist/src/index.d.ts",
  "exports": {
    "types": "./dist/src/index.d.ts",
    "default": "./dist/src/index.js"
  },
  "keywords": [
    "async",
    "context",
    "async-context",
    "zone",
    "tc39",
    "javascript",
    "ecmascript",
    "spec"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tc39/proposal-async-context.git"
  },
  "scripts": {
    "attw": "attw --quiet",
    "fix-esm-import-path": "fix-esm-import-path dist/src/",
    "prepack": "yarn run webpack && yarn run fix-esm-import-path",
    "prepublish": "concurrently --kill-others-on-fail --names attw,publint --prefix-colors auto \"yarn run attw\" \"yarn run publint\"",
    "publint": "publint",
    "webpack": "webpack"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.16.4",
    "@types/mocha": "^10.0.9",
    "@types/node": "^22.7.7",
    "concurrently": "^9.0.1",
    "fix-esm-import-path": "^1.10.1",
    "proposal-async-context": "git://github.com/tc39/proposal-async-context.git",
    "publint": "^0.2.11",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.3",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  }
}
