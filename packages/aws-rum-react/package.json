    "@typescript-eslint/parser": "^7.2.0",
    "aws-rum-web": "^1.17.1",
    "babel-jest": "^29.7.0",
    "babel-plugin-replace-import-extension": "^1.1.4",
    "cjs-ts": "workspace:^",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
