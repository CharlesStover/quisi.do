{
  "name": "@charlesstover/cloudflare-analytics-worker",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "deploy": "wrangler publish src/index.ts --name cloudflare-analytics",
    "dev": "wrangler dev",
    "prepack": "exit 0",
    "prepublish": "wrangler publish --dry-run --outdir dist",
    "start": "concurrently --kill-others --kill-others-on-fail --names dev,tail \"yarn run dev\" \"yarn run tail\"",
    "tail": "wrangler tail --format pretty --ip self cloudflare-analytics"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230115.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.19",
    "concurrently": "^7.6.0",
    "jest": "^29.4.1",
    "typescript": "^4.9.5",
    "wrangler": "2.9.0"
  }
}
