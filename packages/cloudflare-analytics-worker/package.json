{
  "name": "@charlesstover/cloudflare-analytics-worker",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "deploy": "wrangler publish src/index.ts --name cloudflare-analytics --outdir dist",
    "deploy:dry-run": "wrangler publish src/index.ts --dry-run --name cloudflare-analytics --outdir dist",
    "dev": "wrangler dev --test-scheduled",
    "init": "echo \"API token:\" && wrangler secret put API_TOKEN && echo \"Account tag:\" && wrangler secret put ACCOUNT_TAG && echo \"Zone tag:\" && wrangler secret put ZONE_TAG",
    "jest": "jest",
    "jest:watch": "jest --watch",
    "prepack": "exit 0",
    "prepublish": "jest && yarn run deploy:dry-run",
    "tail": "wrangler tail --format pretty --ip self cloudflare-analytics"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230724.0",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.5",
    "jest": "^29.6.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6",
    "wrangler": "^3.3.0"
  }
}
