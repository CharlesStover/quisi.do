{
  "name": "@charlesstover/cloudflare-analytics-worker",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "deploy": "wrangler publish src/index.ts --name cloudflare-analytics",
    "dev": "wrangler dev",
    "dev:local": "wrangler dev --local",
    "prepack": "exit 0",
    "prepublish": "wrangler publish --dry-run --outdir dist",
    "start": "yarn run dev",
    "tail": "wrangler tail --format pretty --ip self cloudflare-analytics"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230404.0",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.3",
    "jest": "^29.5.0",
    "typescript": "^5.0.4",
    "wrangler": "^2.17.0"
  }
}
