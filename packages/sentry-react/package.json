{
  "name": "sentry-react",
  "version": "2.0.0",
  "author": "quisi.do <sentry-react@quisi.do>",
  "description": "A React component for instantiating Sentry",
  "homepage": "https://www.npmjs.com/package/sentry-react",
  "license": "MIT",
  "main": "./dist/index.js",
  "type": "module",
  "types": "./dist/index.d.ts",
  "bugs": {
    "email": "sentry-react@quisi.do",
    "url": "https://github.com/quisido/quisi.do/issues"
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/quisido"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/quisido/quisi.do.git"
  },
  "scripts": {
    "attw": "attw",
    "clean": "rm -rf dist jest && rm -f tsconfig.prepack.tsbuildinfo tsc-output.cpuprofile",
    "eslint": "eslint . --cache --color --exit-on-fatal-error --format @microsoft/eslint-formatter-sarif --max-warnings 0 --output-file ../../sarif/sentry-react.sarif --stats || (echo \"ESLint failed.\" && exit 1)",
    "eslint:fix": "eslint . --cache --color --exit-on-fatal-error --fix --max-warnings 0",
    "jest": "jest --config jest.config.cjs",
    "jest:watch": "jest --config jest.config.cjs --watch",
    "prepack": "yarn run tsc",
    "prepublish": "concurrently --kill-others-on-fail --names attw,eslint,jest \"yarn run attw\" \"yarn run eslint\" \"yarn run jest\"",
    "tsc": "tsc --generateCpuProfile tsc-output.cpuprofile --project tsconfig.prepack.json"
  },
  "dependencies": {
    "@sentry/browser": "^7.111.0",
    "@sentry/react": "^7.111.0",
    "@sentry/types": "^7.111.0",
    "use-shallow-memo": "workspace:^"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.15.3",
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@microsoft/eslint-formatter-sarif": "^3.1.0",
    "@monorepo-template/eslint-config": "^1.0.7",
    "@monorepo-template/jest-transformer": "^1.0.2",
    "@quisido/eslint-config": "workspace:^",
    "@sentry/react": "^7.111.0",
    "@testing-library/react": "^15.0.2",
    "@types/babel__core": "^7.20.5",
    "@types/babel__preset-env": "^7.9.6",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@typescript-eslint/eslint-plugin": "patch:@typescript-eslint/eslint-plugin@npm%3A7.7.0#~/.yarn/patches/@typescript-eslint-eslint-plugin-npm-7.7.0-de43f1e757.patch",
    "@typescript-eslint/parser": "^7.7.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-replace-import-extension": "^1.1.4",
    "cjs-ts": "workspace:^",
    "concurrently": "^8.2.2",
    "eslint": "^9.1.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "ts-jest": "^29.1.2",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  }
}
