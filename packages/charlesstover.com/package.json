  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/CharlesStover"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "scripts": {
    "build": "DISABLE_ESLINT_PLUGIN=true react-app-rewired build",
    "coverage": "monorepo-template-coverage",
    "cypress": "concurrently --hide server --kill-others --kill-others-on-fail --names server,cypress \"yarn run cypress:start\" \"wait-on http://localhost:3000/ && yarn run cypress:open\"",
    "cypress:open": "cypress open",
    "cypress:pnp": "node ./scripts/cypress-pnp.cjs",
    "cypress:run": "cypress run",
    "cypress:start": "BROWSER=none DISABLE_ESLINT_PLUGIN=true react-app-rewired start",
    "eslint": "eslint '**/*'",
    "eslint:fix": "eslint '**/*' --fix",
    "jest": "jest --runInBand",
    "jest:watch": "jest --runInBand --watch",
    "lighthouse": "concurrently --hide server --kill-others --kill-others-on-fail --names server,lighthouse --success command-lighthouse \"yarn run serve\" \"wait-on http://localhost:3000/ && yarn run lighthouse:report\"",
    "lighthouse:report": "lighthouse http://localhost:3000/ --chrome-flags=\"--headless\" --output=html --output-path=./lighthouse.report.html --preset=experimental",
    "prepack": "yarn build",
    "prepublish": "yarn eslint && yarn jest",
    "serve": "serve --single build",
    "start": "DISABLE_ESLINT_PLUGIN=true react-app-rewired start"
  },
