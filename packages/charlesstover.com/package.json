  "author": "Charles Stover <charlesstover.com@charlesstover.com>",
  "config-overrides-path": "./config-overrides.cjs",
  "description": "the professional portfolio of Charles Stover",
  "homepage": ".",
  "private": true,
  "repository": "github:CharlesStover/charlesstover.com",
  "type": "module",
  "bugs": {
    "email": "charlesstover.com@charlesstover.com",
