  "scripts": {
    "build": "DISABLE_ESLINT_PLUGIN=true react-app-rewired build",
    "coverage": "monorepo-template-coverage",
    "cypress": "concurrently --hide server --kill-others --kill-others-on-fail --names server,cypress --success command-cypress \"yarn run cypress:start\" \"yarn run cypress:run\"",
    "cypress:local": "concurrently --hide server --kill-others --kill-others-on-fail --names server,cypress \"yarn run cypress:start\" \"yarn run cypress:open\"",
    "cypress:open": "wait-on http://localhost:3000/ && cypress open",
    "cypress:pnp": "node ./scripts/cypress-pnp/index.js",
    "cypress:run": "wait-on http://localhost:3000/ && cypress run --browser chrome:canary",
    "cypress:start": "BROWSER=none DISABLE_ESLINT_PLUGIN=true react-app-rewired start",
    "e2e": "concurrently --kill-others-on-fail --names cypress,lighthouse \"yarn run cypress\" \"yarn run lighthouse\" && yarn run coverage",
    "eslint": "eslint '**/*' --format @microsoft/eslint-formatter-sarif --output-file ../../sarif/charlesstover-com.sarif || eslint '**/*'",
    "eslint:fix": "eslint '**/*' --fix",
    "jest": "jest --runInBand",
    "jest:watch": "jest --runInBand --watch",
    "lighthouse": "concurrently --hide server --kill-others --kill-others-on-fail --names server,lighthouse --success command-lighthouse \"yarn run serve\" \"yarn run lighthouse:report\"",
    "lighthouse:report": "wait-on http://localhost:3000/index.html && lighthouse http://localhost:3000/ --budget-path=lighthouse.budget.json --chrome-flags=\"--headless\" --config-path=lighthouse.config.js --enable-error-reporting --output=html,json --output-path=lighthouse --preset=experimental",
    "postlighthouse": "yarn node ./scripts/postlighthouse/index.js",
    "prepack": "yarn run build",
    "prepublish": "concurrently --kill-others-on-fail --names eslint,jest \"yarn run eslint\" \"yarn run jest\"",
    "serve": "serve --config ../serve.json --cors --debug --single build",
    "start": "DISABLE_ESLINT_PLUGIN=true REACT_APP_CLOUDFLARE_ANALYTICS_ORIGIN=http://localhost:8787 react-app-rewired start"
  },
