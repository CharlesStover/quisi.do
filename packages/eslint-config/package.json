{
  "name": "@quisido/eslint-config",
  "version": "1.0.0",
  "author": "quisi.do <eslint-config@quisi.do>",
  "description": "ESLint configurations for quisi.do",
  "homepage": "https://github.com/quisido/quisi.do/tree/main/packages/eslint-config#readme",
  "license": "MIT",
  "main": "./dist/esm/index.js",
  "module": "./dist/esm.index.js",
  "repository": "github:quisido/quisi.do",
  "type": "module",
  "types": "./dist/esm/index.d.ts",
  "bugs": {
    "email": "eslint-config@quisi.do",
    "url": "https://github.com/quisido/quisi.do/issues"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./cjs": {
      "import": "./dist/cjs.js",
      "types": "./dist/cjs.d.ts",
      "default": "./dist/cjs.js"
    },
    "./dts": {
      "import": "./dist/dts.js",
      "types": "./dist/dts.d.ts",
      "default": "./dist/dts.js"
    },
    "./js": {
      "import": "./dist/js.js",
      "types": "./dist/js.d.ts",
      "default": "./dist/js.js"
    },
    "./ts": {
      "import": "./dist/ts.js",
      "types": "./dist/ts.d.ts",
      "default": "./dist/ts.js"
    }
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/quisido"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "attw": "attw",
    "clean": "rm -rf dist jest && rm -f tsconfig.prepack.tsbuildinfo tsc-output.cpuprofile",
    "eslint": "eslint . --cache --color --exit-on-fatal-error --format @microsoft/eslint-formatter-sarif --max-warnings 0 --output-file ../../sarif/eslint-config.sarif --stats || (echo \"ESLint failed.\" && exit 1)",
    "eslint:fix": "eslint . --cache --color --exit-on-fatal-error --fix --max-warnings 0",
    "jest": "jest --config jest.config.cjs",
    "jest:watch": "jest --config jest.config.cjs --watch",
    "prepack": "yarn run tsc",
    "prepublish": "concurrently --kill-others-on-fail --names attw,eslint,jest \"yarn run attw\" \"yarn run eslint\" \"yarn run jest\"",
    "tsc": "tsc --generateCpuProfile tsc-output.cpuprofile --project tsconfig.prepack.json"
  },
  "dependencies": {
    "@eslint/js": "^9.1.1",
    "@types/eslint": "^8.56.10",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint__js": "^8.42.3",
    "@typescript-eslint/eslint-plugin": "patch:@typescript-eslint/eslint-plugin@npm%3A7.7.0#~/.yarn/patches/@typescript-eslint-eslint-plugin-npm-7.7.0-de43f1e757.patch",
    "@typescript-eslint/parser": "^7.7.0",
    "typescript-eslint": "patch:typescript-eslint@npm%3A7.7.0#~/.yarn/patches/typescript-eslint-npm-7.7.0-1c6d9ccc41.patch"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.15.3",
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@microsoft/eslint-formatter-sarif": "^3.1.0",
    "@monorepo-template/jest-transformer": "^1.0.2",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "babel-jest": "^29.7.0",
    "babel-plugin-replace-import-extension": "^1.1.4",
    "cjs-ts": "workspace:^",
    "concurrently": "^8.2.2",
    "eslint": "^9.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "eslint": "^9.1.0"
  }
}
