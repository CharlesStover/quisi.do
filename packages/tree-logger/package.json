    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "prettier": "^3.3.3",
    "publint": "^0.2.11",
    "tslib": "^2.8.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.12",
    "vitest": "^2.1.3"
  }
