{
  "name": "lazy-i18n",
  "version": "3.0.0",
  "author": "Quisido <lazy-i18n@quisi.do>",
  "description": "Lazy loads translations for React apps",
  "homepage": "https://www.npmjs.com/package/lazy-i18n",
  "license": "MIT",
  "main": "./dist/index.js",
  "repository": "github:CharlesStover/lazy-i18n",
  "type": "module",
  "types": "./dist/index.d.ts",
  "bugs": {
    "email": "lazy-i18n@charlesstover.com",
    "url": "https://github.com/CharlesStover/lazy-i18n/issues"
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/CharlesStover"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "attw": "attw",
    "eslint": "eslint '**/*' --format @microsoft/eslint-formatter-sarif --output-file ../../sarif/lazy-i18n.sarif || eslint '**/*'",
    "eslint:fix": "eslint '**/*' --fix",
    "jest": "jest",
    "prepack": "yarn run tsc",
    "prepublish": "concurrently --kill-others-on-fail --names attw,eslint,jest \"yarn run attw\" \"yarn run eslint\" \"yarn run jest\"",
    "tsc": "tsc --generateCpuProfile tsc-output.cpuprofile"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.12.2",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.9",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  }
}
