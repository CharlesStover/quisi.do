  "scripts": {
    "attw": "attw",
    "build": "next build",
    "cdk:synth": "cdk synth",
    "coverage": "monorepo-template-coverage",
    "cypress": "concurrently --hide server --kill-others --kill-others-on-fail --names server,cypress --success command-cypress \"next build && next start\" \"yarn run cypress:run\"",
    "cypress:open": "wait-on http://localhost:3000/ && cypress open",
    "cypress:pnp": "node ./scripts/cypress-pnp/index.js",
    "cypress:run": "wait-on http://localhost:3000/ && cypress run --browser chrome:canary",
    "dev": "next dev",
    "eslint": "eslint '**/*' --format @microsoft/eslint-formatter-sarif --output-file ../../sarif/next.sarif",
    "eslint:fix": "eslint '**/*' --fix",
    "export": "next export",
    "jest": "jest",
    "lighthouse": "concurrently --hide server --kill-others --kill-others-on-fail --names server,lighthouse --success command-lighthouse \"yarn run build && yarn run serve\" \"yarn run lighthouse:report\"",
    "lighthouse:report": "wait-on http://localhost:3000/index.html && lighthouse http://localhost:3000/ --budget-path=lighthouse.budget.json --chrome-flags=\"--headless\" --config-path=lighthouse.config.js --enable-error-reporting --output=html,json --output-path=lighthouse --preset=experimental",
    "lint": "next lint",
    "postlighthouse": "yarn node ./scripts/postlighthouse/index.js",
    "prepack": "yarn run build",
    "prepublish": "exit 0",
    "serve": "serve out",
    "start": "REACT_APP_CLOUDFLARE_ANALYTICS_ORIGIN=http://localhost:8787 next start -p 3000"
  },
