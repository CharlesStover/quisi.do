{
  "editor.tabSize": 2,
  "eslint.nodePath": ".yarn/sdks",
  "eslint.runtime": "node",
  "files.eol": "\n",
  "prettier.configPath": ".prettierrc.json",
  "prettier.prettierPath": ".yarn/sdks/prettier/index.cjs",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "typescript.tsdk": ".yarn/sdks/typescript/lib",
  "editor.rulers": [
    80
  ],
  "files.exclude": {
    "**/.git/": true,
    "**/.idea/": true,
    "**/.next/": true,
    "**/.nyc_output/": true,
    "**/.yarn/": true,
    "**/build/": true,
    "**/coverage/": true,
    "**/cypress/coverage/": true,
    "**/cypress/downloads/": true,
    "**/cypress/screenshots/": true,
    "**/cypress/videos/": true,
    "**/dist/": true,
    "**/jest/": true,
    "**/node_modules/": true,
    "**/sarif/": true,
    "**/.attach_pid*": true,
    "**/.pnp.*": true,
    "**/*.cpuprofile": true,
    "**/*.tsbuildinfo": true,
    "**/lighthouse.report.*": true,
    "**/yarn.lock": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.idea/**": true,
    "**/.next/**": true,
    "**/.nyc_output/**": true,
    "**/.yarn/cache/**": true,
    "**/.yarn/sdks/**": true,
    "**/.yarn/unplugged/**": true,
    "**/.yarn/build-state.yml": true,
    "**/.yarn/install-state.gz": true,
    "**/build/**": true,
    "**/coverage/**": true,
    "**/cypress/coverage/**": true,
    "**/cypress/downloads/**": true,
    "**/cypress/screenshots/": true,
    "**/cypress/videos/": true,
    "**/dist/**": true,
    "**/jest/**": true,
    "**/node_modules/**": true,
    "**/.attach_pid*": true,
    "**/.pnp.*": true,
    "**/*.cpuprofile": true,
    "**/*.tsbuildinfo": true,
    "**/lighthouse.report.*": true,
    "**/yarn.lock": true
  },
  "search.exclude": {
    "**/.git/": true,
    "**/.idea/": true,
    "**/.next/": true,
    "**/.nyc_output/": true,
    "**/.yarn/": true,
    "**/build/": true,
    "**/coverage/": true,
    "**/cypress/coverage/": true,
    "**/cypress/downloads/": true,
    "**/cypress/screenshots/": true,
    "**/cypress/videos/": true,
    "**/dist/": true,
    "**/jest/": true,
    "**/node_modules/": true,
    "**/sarif/": true,
    "**/.attach_pid*": true,
    "**/.pnp.*": true,
    "**/*.cpuprofile": true,
    "**/*.tsbuildinfo": true,
    "**/lighthouse.report.*": true,
    "**/yarn.lock": true,
    "**/.yarn": true
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
