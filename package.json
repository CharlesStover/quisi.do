{
  "name": "@quisi/do",
  "version": "1.0.0",
  "author": "Quisido <contact@quisi.do>",
  "description": "Quisido",
  "homepage": "https://quisi.do/",
  "packageManager": "yarn@3.6.4",
  "private": true,
  "repository": "github:CharlesStover/quisi.do",
  "type": "module",
  "bugs": {
    "email": "bugs@quisi.do",
    "url": "https://github.com/CharlesStover/quisi.do/issues"
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/CharlesStover"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "scripts": {
    "attw": "attw",
    "browserslist:update-db": "browserslist --update-db",
    "build": "yarn packages/next run build",
    "cloudflare-analytics": "yarn packages/cloudflare-analytics-worker run dev",
    "cloudflare-analytics:init": "yarn packages/cloudflare-analytics-worker run init",
    "dev": "yarn packages/next run dev",
    "eslint": "eslint '**/*' --format @microsoft/eslint-formatter-sarif --output-file ./sarif/index.sarif || eslint '**/*'",
    "eslint:fix": "eslint '**/*' --fix && yarn workspaces foreach --interlaced --parallel --topological-dev --verbose run eslint:fix",
    "lighthouse": "yarn packages/next run lighthouse",
    "prepack": "yarn workspaces foreach --parallel --topological-dev run prepack",
    "prepublish": "concurrently --kill-others-on-fail --names eslint,prepublish,test \"yarn run eslint\" \"yarn workspaces foreach --interlaced --parallel --topological-dev --verbose run prepublish\" \"yarn run test\"",
    "publish": "yarn workspaces foreach --no-private --parallel --topological-dev --verbose npm publish --tolerate-republish",
    "test": "monorepo-template-test",
    "up": "yarn up \"@*/*\" \"!(aws-rum-react|fullstory-react|lazy-i18n|number-format-react|react-datadog|sentry-react|unknown2string|use-awsui-table-item-description|use-next-awsui|use-offline)\" && yarn up --recursive \"@*/*\" \"*\" && yarn sdks vscode"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.10.2",
    "@microsoft/eslint-formatter-sarif": "^3.0.0",
    "@monorepo-template/eslint-config": "^1.0.7",
    "@monorepo-template/test": "^2.2.0",
    "@monorepo-template/up": "^1.0.1",
    "@types/node": "^20.6.5",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@yarnpkg/sdks": "^3.0.0-rc.51",
    "browserslist": "^4.21.11",
    "concurrently": "^8.2.1",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  }
}
