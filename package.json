{
  "name": "@quisi/do",
  "version": "1.0.0",
  "author": "Chaz Quisido <chaz@quisi.do>",
  "description": "the professional portfolio of Chaz Quisido",
  "homepage": "https://quisi.do/",
  "packageManager": "yarn@3.6.3",
  "private": true,
  "repository": "github:CharlesStover/quisi.do",
  "type": "module",
  "bugs": {
    "email": "chaz@quisi.do",
    "url": "https://github.com/CharlesStover/quisi.do/issues"
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/CharlesStover"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "scripts": {
    "browserslist:update-db": "browserslist --update-db",
    "build": "yarn packages/react-scripts run build",
    "cloudflare-analytics": "yarn packages/cloudflare-analytics-worker run dev",
    "cloudflare-analytics:init": "yarn packages/cloudflare-analytics-worker run init",
    "dev": "yarn packages/next run dev",
    "eslint": "eslint '**/*' --format @microsoft/eslint-formatter-sarif --output-file ./sarif/index.sarif || eslint '**/*'",
    "eslint:fix": "eslint '**/*' --fix",
    "lighthouse": "yarn packages/react-scripts run lighthouse",
    "prepack": "yarn workspaces foreach --parallel --topological-dev run prepack",
    "prepublish": "concurrently --kill-others-on-fail --names eslint,prepublish,test \"yarn run eslint\" \"yarn workspaces foreach --interlaced --parallel --topological-dev --verbose run prepublish\" \"yarn run test\"",
    "publish": "yarn workspaces foreach --no-private --parallel --topological-dev --verbose npm publish --tolerate-republish",
    "serve": "yarn packages/react-scripts run serve",
    "test": "monorepo-template-test",
    "up": "yarn up \"@*/*\" \"!(aws-rum-react|fullstory-react|lazy-i18n|react-datadog|rollup-config|sentry-react|unknown2string|use-next-awsui|use-offline)\" && yarn up --recursive \"@*/*\" \"*\" && yarn sdks vscode"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@microsoft/eslint-formatter-sarif": "^3.0.0",
    "@monorepo-template/eslint-config": "^1.0.7",
    "@monorepo-template/test": "^2.2.0",
    "@monorepo-template/tsconfig": "^1.1.2",
    "@monorepo-template/up": "^1.0.1",
    "@types/node": "^20.6.5",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "@yarnpkg/sdks": "^3.0.0-rc.51",
    "browserslist": "^4.21.11",
    "concurrently": "^8.2.1",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "resolutions": {
    "aws-rum-web@^1.14.0": "patch:aws-rum-web@npm%3A1.14.0#./.yarn/patches/aws-rum-web-npm-1.14.0-653ad283fd.patch",
    "@sentry/react@^7.70.0": "patch:@sentry/react@npm%3A7.70.0#./.yarn/patches/@sentry-react-npm-7.70.0-497abb71d0.patch"
  }
}
