{
  "name": "@charlesstover/charlesstover.com",
  "version": "1.0.0",
  "author": "Charles Stover <charlesstover.com@charlesstover.com>",
  "description": "the professional portfolio of Charles Stover",
  "homepage": "https://charlesstocer.com/",
  "packageManager": "yarn@3.3.0",
  "private": true,
  "repository": "github:CharlesStover/charlesstover.com",
  "type": "module",
  "bugs": {
    "email": "charlesstover.com@charlesstover.com",
    "url": "https://github.com/CharlesStover/charlesstover.com/issues"
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/CharlesStover"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "scripts": {
    "browserslist:update-db": "browserslist --update-db",
    "build": "yarn prepack",
    "eslint": "eslint '**/*'",
    "eslint:fix": "eslint '**/*' --fix",
    "prepack": "yarn workspaces foreach --parallel --topological-dev run prepack",
    "prepublish": "yarn eslint && yarn test && yarn workspaces foreach --no-private --parallel --topological-dev run prepublish",
    "test": "monorepo-template-test",
    "up": "monorepo-template-up"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@monorepo-template/eslint-config": "^1.0.4",
    "@monorepo-template/test": "^2.2.0",
    "@monorepo-template/tsconfig": "^1.1.2",
    "@monorepo-template/up": "^1.0.1",
    "@types/node": "^18.11.14",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "@yarnpkg/sdks": "^3.0.0-rc.33",
    "browserslist": "^4.21.4",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "prettier": "^2.8.1",
    "typescript": "4.9.3"
  }
}
