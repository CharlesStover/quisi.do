{
  "name": "@charlesstover/charlesstover.com",
  "version": "1.0.0",
  "author": "Charles Stover <charlesstover.com@charlesstover.com>",
  "description": "the professional portfolio of Charles Stover",
  "homepage": "https://charlesstocer.com/",
  "packageManager": "yarn@3.5.1",
  "private": true,
  "repository": "github:CharlesStover/charlesstover.com",
  "type": "module",
  "bugs": {
    "email": "charlesstover.com@charlesstover.com",
    "url": "https://github.com/CharlesStover/charlesstover.com/issues"
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/CharlesStover"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "scripts": {
    "browserslist:update-db": "browserslist --update-db",
    "build": "yarn packages/charlesstover.com run build",
    "cloudflare-analytics": "yarn packages/cloudflare-analytics-worker run dev",
    "cloudflare-analytics:init": "yarn packages/cloudflare-analytics-worker run init",
    "eslint": "eslint '**/*' --format @microsoft/eslint-formatter-sarif --output-file ./sarif/index.sarif || eslint '**/*'",
    "eslint:fix": "eslint '**/*' --fix",
    "lighthouse": "yarn packages/charlesstover.com run lighthouse",
    "prepack": "yarn workspaces foreach --parallel --topological-dev run prepack",
    "prepublish": "concurrently --kill-others-on-fail --names eslint,prepublish,test \"yarn run eslint\" \"yarn workspaces foreach --interlaced --parallel --topological-dev --verbose run prepublish\" \"yarn run test\"",
    "publish": "yarn workspaces foreach --no-private --parallel --topological-dev --verbose npm publish --tolerate-republish",
    "serve": "yarn packages/charlesstover.com run serve",
    "start": "concurrently --kill-others --kill-others-on-fail --names aws-rum-react,charlesstover.com,cloudflare-analytics,react-datadog \"yarn packages/aws-rum-react run rollup:watch\" \"yarn packages/charlesstover.com run start\" \"yarn packages/cloudflare-analytics-worker run dev\" \"yarn packages/react-datadog run rollup:watch\"",
    "test": "monorepo-template-test",
    "up": "yarn up \"@*/*\" \"!(aws-rum-react|react-datadog|unknown2string|use-offline)\" && yarn up --recursive \"@*/*\" \"*\" && yarn sdks vscode"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@microsoft/eslint-formatter-sarif": "^3.0.0",
    "@monorepo-template/eslint-config": "^1.0.7",
    "@monorepo-template/test": "^2.2.0",
    "@monorepo-template/tsconfig": "^1.1.2",
    "@monorepo-template/up": "^1.0.1",
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@yarnpkg/sdks": "^3.0.0-rc.44",
    "browserslist": "^4.21.7",
    "concurrently": "^8.0.1",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  },
  "resolutions": {
    "@awsui/components-react@^3.0.808": "patch:@awsui/components-react@npm%3A3.0.808#./.yarn/patches/@awsui-components-react-npm-3.0.808-f471988300.patch",
    "@cloudscape-design/components@^3.0.263": "patch:@cloudscape-design/components@npm%3A3.0.263#./.yarn/patches/@cloudscape-design-components-npm-3.0.263-3574b4acbf.patch"
  }
}
