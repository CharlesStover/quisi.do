{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "noEmit": true
  },
  "exclude": [
    "/.yarn/",
    "/build/",
    "/cypress/",
    "/jest/",
    "/node_modules/",
    "/types/"
  ],
  "include": [
    "**/*.cjs",
    "**/*.d.ts",
    "**/*.js",
    "**/*.json",
    "**/*.ts",
    "**/*.tsx",
    "**/*.yml"
  ]
}
